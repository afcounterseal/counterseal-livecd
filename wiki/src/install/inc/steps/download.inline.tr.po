msgid "This section provides simplified instructions:"
msgstr ""

#. type: Content of: <div><ul><li>
msgid ""
"<a href=\"#windows\">In Windows with <span class=\"application\">Gpg4win</"
"span></a>"
msgstr ""

#. type: Content of: <div><ul><li>
msgid ""
"<a href=\"#mac\">In macOS with <span class=\"application\">GPGTools</span></"
"a>"
msgstr ""

