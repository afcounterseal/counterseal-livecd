#, no-wrap
msgid "<h2 id=\"rtl8723be\">RTL8723BE PCIe Wireless Network Adapter</h2>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"<!--\n"
"Last updated: 2017-12-24\n"
"-->\n"
msgstr ""

